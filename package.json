  "description": "CLI para migrar recursos do Twilio entre contas (Studio Flows, TaskRouter, Serverless, Content Templates)",
  "main": "index.js",
  "bin": {
    "twilio-account-migrate": "dist/index.js"
  },
  "scripts": {
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand",
