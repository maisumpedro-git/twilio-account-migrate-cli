    "inquirer": "^12.9.4",
    "lodash": "^4.17.21",
    "ora": "^8.2.0",
    "twilio": "^5.9.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2"
